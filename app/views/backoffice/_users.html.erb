<table class="table  table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.type_of_users %></td>
        <td><%= user.active %></td>
        <td><%= link_to 'Show', backoffice_user_show_path(user) %></td>
        <td><%= link_to 'Edit', backoffice_user_edit_path(user) %></td>
        <td><%= link_to 'Destroy', backoffice_user_delete_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <%= will_paginate @users, :param_name => 'page' %>
  </tbody>
</table>